<%= form_for @delivery do |f| %>
  <%= f.label :package_size %>
  <%= f.text_field :package_size %>
  <%= f.label :from_city %>
  <%= f.text_field :from_city %>
  <%= f.label :to_city %>
  <%= f.text_field :to_city %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :departure_date %>
  <%= f.datetime_field :departure_date %>
  <%= f.label :arrival_date %>
  <%= f.datetime_field :arrival_date %>
  <%= f.submit %>
<% end %>
